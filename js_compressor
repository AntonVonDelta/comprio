//This is a test image-array

function compress(){
	var fin_array=[];
	var cluster={el:0x00,count:0};

	cluster.el=a[0];
	cluster.count=1;
	for(var i=1;i<uncompressed_image_array.length;i++){
		if(uncompressed_image_array[i]==cluster.el && cluster.count<=254) 
			cluster.count++;
		else{
			fin_array.push(cluster.count);
			fin_array.push(cluster.el);

			cluster.el=uncompressed_image_array[i];
			cluster.count=1;
		}
	}
	fin_array.push(cluster.count);
	fin_array.push(cluster.el);
  //fin_array contains the final compressed array
  //document.getElementById("a").innerHTML=fin_array.toString();
  //document.getElementById("info").innerText=("Final/Total: "+fin_array.length+" / "+a.length + " \r\n"+"Compression rate: "+fin_array.length*100/a.length);
}
compress();
